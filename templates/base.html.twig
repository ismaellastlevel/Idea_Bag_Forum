{# First, we override the error messages in forms #}
{% for key, value in _context %}
    {% if value|instanceOf('Symfony\\Component\\Form\\FormView') %}
        {% form_theme value with 'override.html.twig' %}
    {% endif %}
{% endfor %}
<!DOCTYPE html>
<html lang="{{ app.request.locale  }}">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">
        {% block specific_stylesheets_libs %}{# Ici pour inclure des dépendences css spécifiques à des pages #}{% endblock %}
        {% block specific_stylesheets %}{# Ici pour inclure du custom css spécifique à des pages #}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
        {% block specific_javascript_libs %}{# Ici pour inclure des dépendences js spécifiques à des pages #}{% endblock %}
        {% block specific_javascripts %}{# Ici pour inclure du custom js spécifiques à des pages #}{% endblock %}
        {% block modals %}{# Ici pour inclure les modales générale #}{% endblock %}
    </body>
</html>
